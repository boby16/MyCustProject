<?xml version="1.0" encoding="utf-8"?>
<Page x:Class="Gssy.Capi.View.P_CmbList2" Title="P_CmbList2" Style="{StaticResource WindowBaseStyle}" Loaded="?80?" LayoutUpdated="?99?" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008">
  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="Auto" />
      <RowDefinition Height="Auto" />
      <RowDefinition Height="Auto" />
      <RowDefinition Height="Auto" />
      <RowDefinition Name="RowNote" Height="*" />
      <RowDefinition Height="Auto" />
    </Grid.RowDefinitions>
    <TextBlock Name="txtQuestionTitle" Grid.Row="0" Margin="30,50,30,10" Style="{StaticResource TitleTextStyle}" TextWrapping="Wrap">问题描述</TextBlock>
    <TextBlock Name="txtCircleTitle" Margin="30,10,30,0" Grid.Row="1" Style="{StaticResource TitleTextStyle}" TextBlock.FontSize="40" TextBlock.FontWeight="Bold" TextWrapping="Wrap">123</TextBlock>
    <WrapPanel Name="wrapFill" Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0">
      <WrapPanel Name="wrapFill1" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,20,0,10">
        <TextBlock Name="txtBefore1" Margin="10,0,10,0" Text="" Style="{StaticResource ContentTextStyle}" TextBlock.TextAlignment="Left" VerticalAlignment="Center" />
        <ComboBox Name="cmbSelect1" Width="160" Style="{StaticResource CmbListStyle}" HorizontalAlignment="Left" SelectionChanged="?148?" />
        <TextBlock Name="txtAfter1" Margin="10,0,0,0" Style="{StaticResource ContentTextStyle}" TextBlock.TextAlignment="Left" VerticalAlignment="Center" />
      </WrapPanel>
      <WrapPanel Name="wrapFill2" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,20,0,10">
        <TextBlock Name="txtBefore2" Margin="10,0,10,0" Text="" Style="{StaticResource ContentTextStyle}" TextBlock.TextAlignment="Left" VerticalAlignment="Center" />
        <ComboBox Name="cmbSelect2" Width="90" Style="{StaticResource CmbListStyle}" HorizontalAlignment="Left" SelectionChanged="?149?" />
        <TextBlock Name="txtAfter2" Margin="10,0,0,0" Style="{StaticResource ContentTextStyle}" TextBlock.TextAlignment="Left" VerticalAlignment="Center" />
      </WrapPanel>
    </WrapPanel>
    <TextBlock Name="txtQuestionNote" HorizontalAlignment="Center" Grid.Row="3" Margin="30,0,30,10" TextBlock.FontSize="32" Style="{StaticResource TitleTextStyle}" TextWrapping="Wrap" VerticalAlignment="Top" />
    <ScrollViewer Name="scrollNote" Grid.Row="4" Margin="30,0,30,5" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Auto" VerticalAlignment="Top">
      <StackPanel Name="NoteArea" Orientation="Vertical">
        <WrapPanel Name="wrapButton" Margin="0,0,0,0" VerticalAlignment="Center" Orientation="Vertical" HorizontalAlignment="Center" />
      </StackPanel>
    </ScrollViewer>
    <DockPanel Grid.Row="5" Margin="5,0,5,0">
      <Image Style="{StaticResource ImgLogo}" />
      <TextBlock Name="txtSurvey" Margin="10,0,0,0" Text="" Style="{StaticResource TitleMediumStyle}" VerticalAlignment="Center" />
      <Button Name="btnAttach" Content="{DynamicResource LBtnAddAttach}" Width="160" Margin="5,5,0,5" VerticalAlignment="Top" Style="{StaticResource NavBtnStyle}" Visibility="Collapsed" Click="?85?" />
      <Button Name="btnNav" Content="{DynamicResource LBtnNav}" Style="{StaticResource NavBtnStyle}" Click="?58?" />
    </DockPanel>
  </Grid>
</Page>