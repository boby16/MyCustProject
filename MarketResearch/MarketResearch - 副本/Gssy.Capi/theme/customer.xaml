<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style
		TargetType="{x:Type Image}"
		x:Key="ImgLoading">
		<Setter
			Property="Stretch"
			Value="UniformToFill" />
		<Setter
			Property="HorizontalAlignment"
			Value="Left" />
		<Setter
			Property="VerticalAlignment"
			Value="Center" />
		<Setter
			Property="Height"
			Value="100" />
		<Setter
			Property="Width"
			Value="100" />
		<Setter
			Property="Source"
			Value="/Gssy.Capi;component/Resources/Loading.gif" />
	</Style>
	<Style
		TargetType="{x:Type Image}"
		x:Key="ImgLogo">
		<Setter
			Property="Stretch"
			Value="UniformToFill" />
		<Setter
			Property="HorizontalAlignment"
			Value="Left" />
		<Setter
			Property="VerticalAlignment"
			Value="Center" />
		<Setter
			Property="Height"
			Value="65" />
		<Setter
			Property="Width"
			Value="200" />
		<Setter
			Property="Source"
			Value="/Gssy.Capi;component/Resources/Logo.png" />
	</Style>
	<Style
		TargetType="{x:Type Image}"
		x:Key="ImgLogo2">
		<Setter
			Property="Stretch"
			Value="UniformToFill" />
		<Setter
			Property="HorizontalAlignment"
			Value="Left" />
		<Setter
			Property="VerticalAlignment"
			Value="Center" />
		<Setter
			Property="Height"
			Value="61" />
		<Setter
			Property="Width"
			Value="295" />
		<Setter
			Property="Source"
			Value="/Gssy.Capi;component/Resources/background.png" />
	</Style>
</ResourceDictionary>
